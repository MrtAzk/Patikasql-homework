Soru 3 =-- 1. ID'ye göre Name değiştir
UPDATE employee SET name = 'Updated Name' WHERE id = 1;

-- 2. Name'e göre Email güncelle
UPDATE employee SET email = 'newemail@example.com' WHERE name = 'Jane Smith';

-- 3. Email'e göre doğum tarihi güncelle
UPDATE employee SET birthday = '2000-01-01' WHERE email = 'johndoe@example.com';

-- 4. Birthday'e göre Name güncelle
UPDATE employee SET name = 'Changed Birthday Name' WHERE birthday = '1990-04-12';

-- 5. ID’ye göre Email güncelle
UPDATE employee SET email = 'idupdate@example.com' WHERE id = 3;

Soru 4 =DELETE FROM employee WHERE id = 50;

-- 2. Name'e göre sil
DELETE FROM employee WHERE name = 'Michael Brown';

-- 3. Email'e göre sil
DELETE FROM employee WHERE email = 'janesmith@example.com';

-- 4. Birthday'e göre sil
DELETE FROM employee WHERE birthday = '1985-06-23';

-- 5. Email domain'e göre toplu silme örneği
DELETE FROM employee WHERE email LIKE '%@example.com';
